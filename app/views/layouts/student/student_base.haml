!!!
%html{lang: "en"}
  %head
    %meta{:charset => "utf-8"}
    %meta{:name => "viewport", :content => "width=device-width, initial-scale=1.0"}
    %meta{ :property => "og:title", :content => "Review your internship" }
    %meta{ :property => "og:site_name", :content => "pinpeg" }
    %title pinpeg
    / = javascript_include_tag 'vendor/modernizr'
    = stylesheet_link_tag 'application'
    = javascript_include_tag "application"
    = stylesheet_link_tag 'student/student_account'
    = csrf_meta_tags

  %body
    = render partial: "student/shared/header"
    = yield
    = render partial: "student/shared/footer"
