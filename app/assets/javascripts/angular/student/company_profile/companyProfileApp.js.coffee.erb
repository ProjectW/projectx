companyProfileApp = angular.module 'companyProfileApp', ['ngRoute','companyProfileControllers', 'resources.companies']

companyProfileApp.config ['$routeProvider', ($routeProvider) -> 
  $routeProvider
    .when('/:companyId', 
      templateUrl: '<%= asset_path("angular/student/company_profile/views/profileView") %>',
      controller: 'CompanyProfileCtrl'
    )
]
