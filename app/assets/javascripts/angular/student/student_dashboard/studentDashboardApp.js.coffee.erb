studentDashboardApp = angular.module 'studentDashboardApp', ['ngRoute','studentDashboardControllers']

studentDashboardApp.config ['$routeProvider', ($routeProvider) -> 
  $routeProvider
    .when('/', 
      templateUrl: '<%= asset_path("angular/student/student_dashboard/views/dashboardView") %>',
      controller: 'StudentDashboardCtrl'
    )
    .when('/resumes/:resumeId', 
      templateUrl: '<%= asset_path("angular/student/student_dashboard/views/resumeView") %>',
      controller: 'ResumeCtrl'
    )
]
